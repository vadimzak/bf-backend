{
  "name": "sample-2",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/sample-2",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nx/node:node",
      "options": {
        "buildTarget": "sample-2:build"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/sample-2",
        "main": "apps/sample-2/server.js",
        "tsConfig": "apps/sample-2/tsconfig.app.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/sample-2/jest.config.ts"
      }
    }
  },
  "tags": []
}