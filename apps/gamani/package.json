{
  "name": "gamani",
  "version": "1.0.2",
  "description": "Gamani full-stack application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"ts-node src/server.ts\" \"cd client && npm run dev\"",
    "dev:server": "ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "tsc",
    "build:client": "cd client && npm run build",
    "build:all": "npm run build && npm run build:client",
    "watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "cognito",
    "dynamodb",
    "react",
    "ai"
  ],
  "author": "Vadim Zak",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.857.0",
    "@aws-sdk/client-secrets-manager": "^3.857.0",
    "@aws-sdk/lib-dynamodb": "^3.857.0",
    "@bf-backend/server-core": "file:../../libs/server-core",
    "@google/genai": "^1.12.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.67",
    "@types/uuid": "^9.0.8",
    "aws-jwt-verify": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "typescript": "~5.8.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
